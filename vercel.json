{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/api.js"
    },
    {
      "src": "/src/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "/img/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/api.js": {
      "maxDuration": 10
    }
  }
}
